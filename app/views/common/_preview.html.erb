<% unless @text.blank? %>
  <%#= textilizable @text, :attachments => @attachments, :object => @previewed %>
  <div id="wiki-content-display">
  	<%= textilizable @text, :attachments => @attachments, :object => @previewed %>
  </div>
<% else	%>
  <p class="empty-preview"><%= l(:label_nothing_to_preview) %></p>
<% end %>